ap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n","\n","    for frame_num in range(frame_count):\n","        cap.set(cv2.CAP_PROP_POS_FRAMES, frame_num)\n","\n","        ret, frame = cap.read()\n","\n","        if not ret:\n","            break\n","\n","        x = frame_num % math.floor(frame_width / 200) * 200\n","        y = frame_num % math.floor(frame_height / 200) * 200\n","\n","        cropped_frame = frame[y:y+200, x:x+200]\n","\n","        output_image_path = os.path.join(output_folder, f\"{video_name_without_extension}frame{frame_num}.jpg\")\n","        cv2.imwrite(output_image_path, cropped_frame)\n","\n","video_path='/content/runs/detect/predict/ABA Therapy - Learning about Animals.avi'\n","output_folder='/content/extracted_frames'\n","extract_frames(video_path, output_folder)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"2SqQ0qXNrRTt"},"outputs":[],"source":["from google.colab.patches import cv2_imshow"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"BoNIR0iUeWFW"},"outputs":[],"source":["import cv2\n","from keras.models import load_model\n","import numpy as np\n","import os\n","from google.colab.patches import cv2_imshow\n","\n","# Load the emotion recognition model\n","emotion_model = load_model('model.h5')\n","\n","# Function to detect emotions in video frames with detected objects\n","def detect_emotions_with_objects(frames_folder):\n","    frame_files = os.listdir(frames_folder)\n","    for frame_file in frame_files:\n","        frame_path = os.path.join(frames_folder, frame_file)\n","        frame = cv2.imread(frame_path)\n","\n","        # Perform emotion recognition on the frame\n","        # Preprocess frame for emotion recognition (assuming it's already preprocessed for YOLOv8 detection)\n","        face = cv2.resize(frame, (48, 48))\n","        face = cv2.cvtColor(face, cv2.COLOR_BGR2GRAY)\n","        face = face / 255.0\n","        face = np.expand_dims(face, axis=0)\n","        face = np.expand_dims(face, axis=-1)\n","\n","        # Predict emotion using the emotion recognition model\n","        emotion_prediction = emotion_model.predict(face)\n","        emotion_label = np.argmax(emotion_prediction)\n","        emotion_label = str(emotion_label)  # Convert emotion_label to string\n","\n","        # Draw emotion label on the frame\n","        color = (0, 255, 0)  # Green color for bounding box\n","        cv2.putText(frame, \"Emotion: \" + emotion_label, (10, 30), cv2.FONT_HERSHEY_SIMPLEX, 1, color, 2)\n","\n","        # Display the frame (or save it to another folder)\n","        cv2_imshow(frame)\n","\n","        if cv2.waitKey(1) & 0xFF == ord('q'):\n","            break\n","\n","    cv2.destroyAllWindows()\n","\n","# Example usage\n","frames_folder = '/content/extracted_frames'\n","detect_emotions_with_objects(frames_folder)\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"JH4qQq3HjLhL"},"outputs":[],"source":["import cv2\n","from keras.models import load_model\n","import numpy as np\n","import os\n","\n","# Load the emotion recognition model\n","emotion_model = load_model('model.h5')\n","\n","# Function to perform emotion recognition on a frame and add the emotion label\n","def add_emotion_label(frame):\n","    # Preprocess frame for emotion recognition\n","    face = cv2.resize(frame, (48, 48))\n","    face = cv2.cvtColor(face, cv2.COLOR_BGR2GRAY)\n","    face = face / 255.0\n","    face = np.expand_dims(face, axis=0)\n","    face = np.expand_dims(face, axis=-1)\n","\n","    # Predict emotion using the emotion recognition model\n","    emotion_prediction = emotion_model.predict(face)\n","    emotion_label = np.argmax(emotion_prediction)\n","\n","    # Add emotion label text to the frame\n","    emotion_text = \"Emotion: \" + str(emotion_label)\n","    cv2.putText(frame, emotion_text, (10, 30), cv2.FONT_HERSHEY_SIMPLEX, 1, (0, 255, 0), 2)\n","\n","    return frame\n","\n","# Function to process each frame of the video, add emotion labels, and write to a new video file\n","def create_video_with_emotion_labels(input_video_path, output_video_path):\n","    cap = cv2.VideoCapture(input_video_path)\n","    frame_width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n","    frame_height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n","    fps = int(cap.get(cv2.CAP_PROP_FPS))\n","\n","    # Define the codec and create VideoWriter object\n","    fourcc = cv2.VideoWriter_fourcc(*'XVID')\n","    out = cv2.VideoWriter(output_video_path, fourcc, fps, (frame_width, frame_height))\n","\n","    while cap.isOpened():\n","        ret, frame = cap.read()\n","        if not ret:\n","            break\n","\n","        # Add emotion label to the frame\n","        frame_with_label = add_emotion_label(frame)\n","\n","        # Write the frame with emotion label to the output video\n","        out.write(frame_with_label)\n","\n","    # Release the VideoCapture and VideoWriter objects\n","    cap.release()\n","    out.release()\n","\n","# Example usage\n","input_video_path = '/content/ABA Therapy - Learning about Animals.avi'\n","output_video_path = '/content/annotated_video_with_emotion_labels.avi'\n","create_video_with_emotion_labels(input_video_path, output_video_path)\n"]},{"cell_type":"code","source":["import cv2\n","from keras.models import load_model\n","import numpy as np\n","import os\n","\n","# Define the emotion labels\n","emotion_labels = ['Angry', 'Disgust', 'Fear', 'Happy', 'Sad', 'Surprise', 'Neutral']\n","\n","# Load the emotion recognition model\n","emotion_model = load_model('model.h5')\n","\n","# Function to perform emotion recognition on a frame and add the emotion label\n","def add_emotion_label(frame):\n","    # Preprocess frame for emotion recognition\n","    face = cv2.resize(frame, (48, 48))\n","    face = cv2.cvtColor(face, cv2.COLOR_BGR2GRAY)\n","    face = face / 255.0\n","    face = np.expand_dims(face, axis=0)\n","    face = np.expand_dims(face, axis=-1)\n","\n","    # Predict emotion using the emotion recognition model\n","    emotion_prediction = emotion_model.predict(face)\n","    emotion_index = np.argmax(emotion_prediction)\n","\n","    # Get the corresponding emotion label\n","    emotion_label = emotion_labels[emotion_index]\n","\n","    # Add emotion label text to the frame\n","    emotion_text = \"Emotion: \" + emotion_label\n","    cv2.putText(frame, emotion_text, (10, 30), cv2.FONT_HERSHEY_SIMPLEX, 1, (0, 255, 0), 2)\n","\n","    return frame\n","\n","# Function to process each frame of the video, add emotion labels, and write to a new video file\n","def create_video_with_emotion_labels(input_video_path, output_video_path):\n","    cap = cv2.VideoCapture(input_video_path)\n","    frame_width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n","    frame_height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n","    fps = int(cap.get(cv2.CAP_PROP_FPS))\n","\n","    # Define the codec and create VideoWriter object\n","    fourcc = cv2.VideoWriter_fourcc(*'XVID')\n","    out = cv2.VideoWriter(output_video_path, fourcc, fps, (frame_width, frame_height))\n","\n","    while cap.isOpened():\n","        ret, frame = cap.read()\n","        if not ret:\n","            break\n","\n","        # Add emotion label to the frame\n","        frame_with_label = add_emotion_label(frame)\n","\n","        # Write the frame with emotion label to the output video\n","        out.write(frame_with_label)\n","\n","    # Release the VideoCapture and VideoWriter objects\n","    cap.release()\n","    out.release()\n","\n","# Example usage\n","input_video_path = '/content/ABA Therapy - Learning about Animals.avi'\n","output_video_path = '/content/annotated_video_with_emotion_labels.avi'\n","create_video_with_emotion_labels(input_video_path, output_video_path)\n"],"metadata":{"id":"GEl3dGpzA773"},"execution_count":null,"outputs":[]},{"cell_type":"code","execution_count":null,"metadata":{"id":"3C6IUibStClX"},"outputs":[],"source":[]}]}